<h3>Mixed page or go back to {{#link-to 'collections' href=false}} {{link-to 'Demo Collections' 'collections' class="btn btn-primary btn-md"}}{{/link-to}}</h3>
<div class="mixed" style="position:relative;height:500px">
  {{#ember-collection items=model estimated-height=800 estimated-width=500 buffer=10 cell-layout=(mixed-grid-layout model) as |item index|}}
    <div class="list-item">
      {{item.name}}
    </div>
  {{/ember-collection}}
</div>
